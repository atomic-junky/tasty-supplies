{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data", 
    "cases": [
      {
        "when": "tasty_supplies/nether_salad",
        "model": {
          "type": "minecraft:model",
          "model": "tasty_supplies:item/nether_salad"
        }
      },
      {
        "when": "tasty_supplies/fruit_salad",
        "model": {
          "type": "minecraft:model",
          "model": "tasty_supplies:item/fruit_salad"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/beetroot_soup"
    }
  }
}