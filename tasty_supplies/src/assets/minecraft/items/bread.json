{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data", 
    "cases": [
      {
        "when": "tasty_supplies/pie_crust",
        "model": {
          "type": "minecraft:model",
          "model": "tasty_supplies:item/pie_crust"
        }
      },
      {
        "when": "tasty_supplies/egg_sandwich",
        "model": {
          "type": "minecraft:model",
          "model": "tasty_supplies:item/egg_sandwich"
        }
      },
      {
        "when": "tasty_supplies/ice_cream_cone",
        "model": {
          "type": "minecraft:model",
          "model": "tasty_supplies:item/ice_cream_cone"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/bread"
    }
  }
}